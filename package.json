{
  "name": "@lightweight-charts-pro/svelte",
  "version": "0.1.0",
  "description": "Svelte components for TradingView Lightweight Charts with backend API integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nandkapadia/svelte-lightweight-charts-pro.git"
  },
  "keywords": [
    "svelte",
    "lightweight-charts",
    "tradingview",
    "charts",
    "financial",
    "trading",
    "typescript",
    "websocket"
  ],
  "peerDependencies": {
    "lightweight-charts": "^5.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "optionalDependencies": {
    "@lightweight-charts-pro/core": "file:../lightweight-charts-pro-frontend"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^24.5.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "globals": "^15.0.0",
    "jsdom": "^25.0.0",
    "lightweight-charts": "^5.0.8",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.17.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "scripts": {
    "build": "vite build && svelte-package",
    "build:types": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src",
    "package": "svelte-package"
  }
}
